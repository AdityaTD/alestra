{
  "name": "smii",
  "version": "2.1.0",
  "description": "Canvas-Constructor helper bot",
  "main": "src/Smii.mjs",
  "scripts": {
    "lint": "eslint --fix src",
    "pm2:delete": "pm2 delete Smii",
    "pm2:logs": "pm2 logs Smii --lines=200",
    "pm2:restart": "pm2 restart Smii",
    "pm2:start-harmony": "pm2 start start.js --node-args=\"--experimental-modules --harmony\" --name=\"Smii\"",
    "pm2:start": "pm2 start start.js --node-args=\"--experimental-modules\" --name=\"Smii\"",
    "pm2:stop": "pm2 stop Smii",
    "start": "node --experimental-modules --experimental-worker src/Smii.mjs",
    "test": "eslint src",
    "update": "npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kyranet/Smii.git"
  },
  "keywords": [
    "Canvas-Constructor",
    "Canvas",
    "helper",
    "discord.js",
    "klasa"
  ],
  "contributors": [
    "kyranet",
    "yorkaargh"
  ],
  "private": true,
  "author": "kyraNET",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kyranet/Smii/issues"
  },
  "homepage": "https://github.com/kyranet/Smii#readme",
  "dependencies": {
    "canvas-constructor": "github:kyranet/canvasConstructor",
    "canvas": "github:Automattic/node-canvas#92b192447e9b9ae98da0f801e4e34afdd1dc1ef8",
    "discord.js": "github:discordjs/discord.js",
    "eslint": "^5.6.0",
    "klasa": "github:dirigeants/klasa#esm",
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "eslint-config-klasa": "github:dirigeants/klasa-lint"
  }
}
